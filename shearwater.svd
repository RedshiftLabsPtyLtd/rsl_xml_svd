            <field>
              <name>POSE_RATE</name>
              <description>Specifies the desired broadcast rate for Redshift Labs NMEA-style pose (Euler Angle/position) packet.</description>
              <bitRange>[27:24]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
